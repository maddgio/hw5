    if (grade >= 90) {
        console.log(`A`);
    } else {
        console.log(`Not A`);
    }

    for (let i = 0; i < 20; i++) {
        console.log(i);
    }

    let stu = {
        name: "maddy",
        id: "s123",
        age: 19,
        grade: 96.6,
        getInfo: function () {
            return this.id;
        }
    };

    console.log(stu.getInfo());
    console.log(stu.age);

    function divide(d1, d2) {
        return d1 / d2;
    }

    function add(n1, n2) {
        return n1 + n2;
    }

    let f2;
    f2 = (m1, m2) => m1 * m2;

    let r = add(2, 3);
    r = divide(6, 2);
    r = f2(10, 2);
    console.log(r);

    const name = "Madison Giovenco";
    console.log(name);

    const weight = 128.8;
    console.log(weight);

    {
        let age2 = 20; // Local
        var age3 = 19;
    }

    console.log(age3);

    let age = 20;
    console.log(age);

    let i = 20;

    if (i > 20) {
        console.log("It is greater than 20");
    } else {
        console.log("It is not greater than 20");
    }

    let str = `The number is ${i}`;
    console.log(str);

    console.log('Hello world');
