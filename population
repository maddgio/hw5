<script>
    let x = sumAll(275638,952997,466103,524907,95634,151356,849477,306601,703366,129777,380688,861418,644098,511151,655735,513936,65117,346875,146084,69815,110926);

    function sumAll() {
        let sum = 0;
        for (let i = 0; i < arguments.length; i++) {
            sum += arguments[i];
        }
        return sum;
    }

    // Display the result in the console
    console.log("The sum is: " + x);
</script>

